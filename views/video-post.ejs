			  <!-- <div id="form-message-success" class="mb-4">
			Your message was sent, thank you!
			  </div> -->
					<form action="/videos/create" enctype="multipart/form-data" method="POST" id="contactForm" class="contactForm">
						<div class="row">
						
						
							<div class="col-md-12">
								<div class="form-group">
									<label class="label" for="subject">Title</label>
									<input type="text" class="form-control" name="title" id="subject" placeholder="What is the title of the video" required>
								</div>
							</div>
							<div class="col-md-12">
								<div class="form-group">
									<label class="label" for="#">Description</label>
									<textarea name="description" class="form-control" id="message" cols="30" rows="4" placeholder="Please describe youe video" required></textarea>
								</div>
							</div>
							<div class="col-md-12 pt-3">
							
								<div class="custom-file">
									<input type="file" class="custom-file-input" id="images" accept="image/*" name="imgUrl" >
									<label class="custom-file-label text-muted" for="images">Choose a thumbnail</label>
								  </div>
							</div>
							<div class="col-md-12 pt-3">
							
									<div class="custom-file">
										<input type="file" class="custom-file-input" id="vide0" accept="video/*" name="videoUrl" required>
										<label class="custom-file-label text-muted" for="images">Choose a video</label>
									  </div>
								</div>
								<div class="col-md-12 pt-3">
									<div class="tag-wrapper">
										<input class="form-control" type="textbox" id="textBox" placeholder="Enter tags"></input>
									</div>
									
								<br/>
								<div class="target" contenteditable="false"></div>
								</div>
								
							<div class="col-md-12 text-center py-5">
								<div class="form-group">
									<input type="submit" value="Upload" class="btn btn-primary">
									<div class="submitting"></div>
								</div>
							</div>
						</div>
					</form>
				</div>
			</div>
			<div class="col-md-5 ">
